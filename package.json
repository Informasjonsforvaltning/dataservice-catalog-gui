{
  "name": "dataservice-catalog-gui",
  "version": "1.0.0",
  "description": "An application for managing data services",
  "private": true,
  "engines": {
    "node": ">= 12.13.0"
  },
  "scripts": {
    "start": "run-s start:webpack",
    "start:webpack": "webpack-dev-server --hot --inline --config-register @babel/register --config webpack/dev.config.js",
    "build:dev": "run-s build:webpack:dev",
    "build:prod": "run-s build:webpack:prod",
    "build:webpack:dev": "webpack -r @babel/register --config webpack/dev.config.js",
    "build:webpack:prod": "webpack -r @babel/register --config webpack/prod.config.js",
    "build:webpack:stats": "webpack -r @babel/register --config webpack/stats.config.js",
    "type:check": "tsc",
    "lint:project": "eslint --ext tsx,ts,jsx,js,json,md,html ./",
    "lint:project:fix": "eslint --ext tsx,ts,jsx,js,json,md,html ./ --fix",
    "lint:staged:fix": "eslint --ext tsx,ts,jsx,js,json,md,html --fix",
    "test": "run-s clean:coverage test:jest",
    "test:verbose": "run-s clean:coverage test:jest:verbose",
    "test:coverage": "run-s clean:coverage test:jest:coverage",
    "test:jest": "jest",
    "test:jest:verbose": "jest --verbose",
    "test:jest:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:watch:all": "jest --watchAll",
    "test:watch:verbose": "jest --watch --verbose",
    "test:watch:all:verbose": "jest --watchAll --verbose",
    "clean:coverage": "rm -rf coverage",
    "bundle:analyse": "run-s build:webpack:stats",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Informasjonsforvaltning/dataservice-catalog-gui.git"
  },
  "author": "German Poljakov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Informasjonsforvaltning/dataservice-catalog-gui/issues"
  },
  "homepage": "https://github.com/Informasjonsforvaltning/dataservice-catalog-gui#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.9.8",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "fast-json-patch": "^3.0.0-1",
    "formik": "^2.1.4",
    "immutable": "^4.0.0-rc.12",
    "keycloak-js": "^9.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-localization": "^1.0.15",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "^0.13.5",
    "sanitize-html": "^1.22.1",
    "showdown": "^1.9.1",
    "styled-components": "^5.0.1",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "@types/jest": "^25.1.4",
    "@types/jwt-decode": "^2.2.1",
    "@types/react": "^16.9.29",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.0",
    "@types/sanitize-html": "^1.22.0",
    "@types/showdown": "^1.9.3",
    "@types/styled-components": "^5.0.1",
    "@types/webpack-env": "^1.15.1",
    "@types/yup": "^0.26.33",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "base-href-webpack-plugin": "^2.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.0.3",
    "copy-webpack-plugin": "^5.1.1",
    "cz-conventional-changelog": "^3.1.0",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "jest": "^25.2.4",
    "lint-staged": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "react-svg-loader": "^3.0.3",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^25.3.0",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  }
}
